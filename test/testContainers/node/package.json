  "license": "MIT",
  "scripts": {
    "clean": "rm -rf ./dist ./native/build",
    "tsc": "tsc --build tsconfig.json",
    "tsc-ui": "tsc --build ./config-ui/tsconfig.test.json",
    "tsc-ci": "tsc --build tsconfig.test.json && cp -R src/sluice/__tests__/data/ dist/sluice/__tests__/ && tsc --build ./config-ui/tsconfig.test.json",
    "dev-sluice": "NODE_ENV=dev ./scripts/build-index-html.sh && BAD_FENCES=1 NODE_ENV=dev webpack --watch --config  webpack.config.sluice.js",
    "dev-dist": "NODE_ENV=dev ./scripts/build-index-html.sh && BAD_FENCES=1 NODE_ENV=dev CRIBL_DEV_DIST=1 webpack --watch --config  webpack.config.sluice.js",
    "dev-dist-clean": "./scripts/dev-dist/clean.sh",
    "dev-sluice-debug": "NODE_ENV=dev ./scripts/build-index-html.sh && NODE_ENV=debug webpack --watch --config  webpack.config.sluice.js",
    "dev-docker": "NODE_ENV=dev ./scripts/build-index-html.sh && FORCE_NATIVE_BUILD=1 webpack --config webpack.config.sluice.js && FORCE_NATIVE_BUILD=1 ./apps/standalone/buildit.sh && ./scripts/deploy-docker.sh ./dist/ local",
    "dev-docker-x64": "NODE_ENV=dev ./scripts/build-index-html.sh && FORCE_NATIVE_BUILD=1 webpack --config webpack.config.sluice.js && ./apps/standalone/buildit.sh x64 && ./scripts/deploy-docker.sh ./dist/ local",
    "dist-build-ui": "./scripts/build-index-html.sh",
    "dist-build-backend": "webpack --config  webpack.config.sluice.js",
    "dist-build-profile": "webpack --profile --json --config webpack.config.sluice.js > dist/profile.json",
    "dist-build": "npm run dist-build-ui && npm run dist-build-backend",
    "dist-splunk-app": "npm run dist-build && ./apps/splunk/buildit.sh",
    "dist-standalone": "npm run dist-build && ./apps/standalone/buildit.sh",
    "dist-standalone-x64": "npm run dist-build && ./apps/standalone/buildit.sh x64",
    "dist-all": "npm run dist-build && ./apps/splunk/buildit.sh && ./apps/standalone/buildit.sh",
    "test-base": "NODE_ENV=test ts-node scripts/run-tests.js run base",
    "test-base-cov": "NODE_ENV=test nyc ts-node scripts/run-tests.js run base",
    "test-expr": "NODE_ENV=test ts-node scripts/run-tests.js run expr",
    "test-share": "NODE_ENV=test ts-node scripts/run-tests.js run share",
    "test-sluice": "native/buildit.sh x64 && NODE_ENV=test ts-node scripts/run-tests.js run sluice",
    "test-sluice-cov": "native/buildit.sh x64 && NODE_ENV=test nyc ts-node scripts/run-tests.js run sluice",
    "test-ui": "NODE_ENV=test jest --runInBand --config ./config-ui/jest.config.js --watchAll=false",
    "test-ui-cov": "NODE_ENV=test jest --config ./config-ui/jest.config.js --coverage --watchAll=false",
    "test-build-backend": "NODE_ENV=test webpack --config webpack.config.sluice.js",
    "test-rest": "npm run test-build-backend && NODE_ENV=test ts-node scripts/run-tests.js run -m 'sluice/__tests__/rest/*test-*'",
    "test-rest-only": "npm run test-build-backend && NODE_ENV=test ts-node scripts/run-tests.js run -m 'sluice/__tests__/rest/test-*'",
    "test-rest-cov": "npm run test-build-backend && NODE_ENV=test nyc --temp-directory .nyc_output_rest00 ts-node scripts/run-tests.js run -m 'sluice/__tests__/rest/*test-*'",
    "test-all": "native/buildit.sh x64 && node scripts/parallel-npm-run.js \"test-base,test-share,test-expr,test-ui,test-sluice\" \"test-rest\"",
    "gen-cov": "mv .nyc_output_rest*/* .nyc_output/ && nyc report",
    "test-all-cov": "native/buildit.sh x64 && NODE_ENV=test nyc ts-node scripts/run-tests.js run base,expr,sluice,share; npm run test-rest-cov; npm run gen-cov",
    "test-package": "src/package/setup.sh && NODE_ENV=test node_modules/.bin/mocha 'src/package/__tests__/*.ts'",
    "dev-ui": "NODE_ENV=development webpack-dev-server --host 0.0.0.0  --open --hot --progress --config ./config-ui/webpack.dev.js",
    "build-ui": "NODE_ENV=production webpack --progress --config ./config-ui/webpack.prod.js",
    "analyze-ui": "NODE_ENV=production ANALYZE_UI=1 webpack --progress --config ./config-ui/webpack.prod.js",
    "gen-swagger": "ts-node --project swagger/tsconfig.json swagger/gen.ts",
    "dev-gen-swagger": "npm run gen-swagger && mkdir -p dist/sluice/default/ui/static/swagger && cp dist/swagger/openapi.json dist/sluice/default/ui/static/swagger/openapi.json",
    "good-fences": "gf",
    "gen-docs": "rm -rf dist/docs && node docs/gendocs.js",
    "dev-gen-docs": "rm -rf dist/docs && node docs/gendocs.js && rm -rf dist/sluice/default/ui/static/docs && cp -r dist/docs dist/sluice/default/ui/static/docs",
    "gen-docs-yml": "ts-node -r tsconfig-paths/register --project docs/tsconfig.json docs/gendocs-yml.ts"
  },
