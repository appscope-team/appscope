{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://appscope.dev/docs/schemas/definitions/body.schema.json",
  "title": "Schema for the AppScope event/metric body",
  "description": "Metadata about the process in which AppScope is running.",
  "$defs": {
    "_time": {
      "title": "_time",
      "description": "_time MEANING WHAT",
      "type": "number",
      "examples": ["1643662126.91777"]
    },
    "_value": {
      "title": "_value",
      "description": "_value OF WHAT",
      "type": "number",
      "examples": [1]
    },
    "args": {
      "title": "args",
      "description": "args TO WHAT? MY GUESS: The arguments with which the scoped command was run.",
      "type": "string"
    },
    "cmd": {
      "title": "cmd",
      "description": "cmd MEANING, THE SCOPED COMMAND?",
      "type": "string",
      "examples": ["top"]
    },
    "data": {
      "title": "data",
      "description": "data MEANING, THE DATA THAT THE SCOPED COMMAND OUTPUT ... ? ",
      "type": "string"
    },
    "gid": {
      "title": "gid",
      "description": "gid OF WHAT",
      "type": "integer",
      "examples": [0]
    },
    "groupname": {
      "title": "groupname",
      "description": "groupname REFERRING TO WHAT KIND OF GROUP",
      "type": "string",
      "examples": ["root"]
    },
    "metric_type_counter": {
      "title": "counter",
      "description": "Indicates that the metric is a counter.",
      "type": "string",
      "const": "counter"
    },
    "metric_type_gauge": {
      "title": "gauge",
      "description": "Indicates that the metric is a gauge.",
      "type": "string",
      "const": "gauge"
